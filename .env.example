# RPC Configuration
FOUNDRY_WRAP_RPC__URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Safe Configuration
FOUNDRY_WRAP_SAFE__SAFE_ADDRESS=0x1234567890123456789012345678901234567890
FOUNDRY_WRAP_SAFE__PROPOSER=0xabcdef1234567890abcdef1234567890abcdef12
FOUNDRY_WRAP_SAFE__CHAIN_ID=1

# Interface Settings
FOUNDRY_WRAP_INTERFACES__GLOBAL_PATH=~/.foundry-wrap/interfaces
FOUNDRY_WRAP_INTERFACES__LOCAL_PATH=interfaces
FOUNDRY_WRAP_INTERFACES__OVERWRITE=false

# Cache Settings
FOUNDRY_WRAP_CACHE__PATH=~/.foundry-wrap/interface-cache.json
FOUNDRY_WRAP_CACHE__ENABLED=true

# Etherscan API Key (special case - no section delimiter)
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# Chain ID for Safe transactions
CHAIN_ID=1