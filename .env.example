# RPC Configuration
SAFESMITH_RPC__URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Safe Configuration
SAFESMITH_SAFE__SAFE_ADDRESS=0x1234567890123456789012345678901234567890
SAFESMITH_SAFE__PROPOSER=0xabcdef1234567890abcdef1234567890abcdef12
SAFESMITH_SAFE__CHAIN_ID=1

# Interface Settings
SAFESMITH_INTERFACES__GLOBAL_PATH=~/.safesmith/interfaces
